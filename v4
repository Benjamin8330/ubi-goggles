-- src download 
curl -s http://ftp.iij.ad.jp/pub/linux/centos-vault/8.3.2011/BaseOS/Source/SPackages/ |   grep "<a href" |   sed -E 's/.*href="([^"]*\.rpm)".*/ftp\.iij\.ad\.jp\/pub\/linux\/centos-vault\/8\.3\.2011\/BaseOS\/Source\/SPackages\/\1/' |   xargs -n 1 wget -P src


