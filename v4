configure: error: mspack >= 0.0.20040308alpha is required.
	fixed by adding powertool repo

---- install VNC server
dnf install -y *vnc*
adduser vncuser
passwd vncuser

echo "[Unit]" >> /etc/systemd/system/vncserver@:1.service
echo "Description=Remote desktop service (VNC)" >> /etc/systemd/system/vncserver@:1.service
echo "After=syslog.target network.target" >> /etc/systemd/system/vncserver@:1.service
echo "" >> /etc/systemd/system/vncserver@:1.service
echo "[Service]" >> /etc/systemd/system/vncserver@:1.service
echo "Type=forking" >> /etc/systemd/system/vncserver@:1.service
echo "User=vncuser" >> /etc/systemd/system/vncserver@:1.service
echo "Group=vncuser" >> /etc/systemd/system/vncserver@:1.service
echo "WorkingDirectory=/home/vncuser" >> /etc/systemd/system/vncserver@:1.service
echo "" >> /etc/systemd/system/vncserver@:1.service
echo "ExecStart=/usr/bin/vncserver :1 -geometry 1280x1024 -depth 24" >> /etc/systemd/system/vncserver@:1.service
echo "ExecStop=/usr/bin/vncserver -kill :1" >> /etc/systemd/system/vncserver@:1.service
echo "" >> /etc/systemd/system/vncserver@:1.service
echo "[Install]" >> /etc/systemd/system/vncserver@:1.service
echo "WantedBy=multi-user.target" >> /etc/systemd/system/vncserver@:1.service
echo "" >> /etc/systemd/system/vncserver@:1.service

systemctl daemon-reload
su vncuser
vncpasswd
exit
systemctl start vncserver@:1
systemctl enable vncserver@:1
systemctl status vncserver@:1
firewall-cmd --permanent --add-port=5901/tcp
firewall-cmd --reload


